<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:prism="http://prismlibrary.com"
             xmlns:i18n="clr-namespace:Resx;assembly=Resx" 
             prism:ViewModelLocator.AutowireViewModel="True"
             x:Class="Covid_Info.Views.TemplatePage"
             Style="{StaticResource PageStyle}">
    <ContentPage.ToolbarItems>
        <ToolbarItem IconImageSource="update" Command="{Binding UpdateData}"/>
        <ToolbarItem Text="COVIDINFO?"  Command="{Binding navDevInfo}" Order="Secondary"/>
    </ContentPage.ToolbarItems>
</ContentPage>